{
    "receipt_content": {
        "agent_type": "${receipt.agentType?lower_case}",
        "client_email_or_phone": "firm1@yandex-team.ru",
        "firm_inn": "${receipt.firm.inn}",
        "firm_reply_email": "firm1@yandex-team.ru",
        "firm_url": "string",
        "additional_document_requisite": "string",
        "additional_user_requisite": {
            "name": "trust_id",
            "value": "06e62000-9cc1-436c-a6db-d64469fa3ab4"
        },
        <#if !receipt.fiscalDocumentType.isEmpty()>"fiscal_doc_type": "${receipt.fiscalDocumentType.get()}",</#if>

        "payments": [
        <#list receipt.payments as payment>
            {
            "amount": "${payment.amount?c}",
            "payment_type": "${payment.type?lower_case}"
            }<#sep>,

        </#list>

        ],
        "receipt_type": "${receipt.receiptType?lower_case}",
        "rows": [
        <#list receipt.rows as row>
            {
                "payment_type_type": "${row.paymentTypeType?lower_case}",
                "price": "${row.price?c}",
                "qty": "${row.qty?c}",
                "tax_type": "${row.taxType?lower_case}",
                "text": "string 666"
            }
            <#sep>,
        </#list>
        ],
        "supplier_phone": "+766699944",
        "taxation_type": "${receipt.taxationType.value}"
    }
}
