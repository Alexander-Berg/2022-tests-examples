<#setting number_format="0.00">
{
    "receipt_calculated_content": {
        "total": "${receipt.totalSum?c}",
        "money_received_total": "${receipt.totalSum?c}",
        "rows": [
            <#list receipt.rows as row>
            {
            "payment_type_type": "${row.paymentTypeType?lower_case}",
            "price": "${row.price?c}",
            "qty": "${row.qty?c}",
            "tax_type": "${row.taxType?lower_case}",
            "text": "string 666",
            "amount": "${row.amount}",
            "tax_pct": "${row.taxType.pct}",
            "tax_amount": "${row.taxAmount}"
            }
                <#sep>,
            </#list>
        ],
        "tax_totals": [
            <#list receipt.taxTotals as taxTotal>
            {
            "tax_type": "${taxTotal.taxType?lower_case}",
            "tax_pct": "${taxTotal.taxType.pct}",
            "tax_amount": "${taxTotal.taxAmount}"
            }
                <#sep>,
            </#list>
        ],
        "totals": [
            {
            "payment_type": "${receipt.paymentType?lower_case}",
            "amount": "${receipt.totalSum?c}"
            }
        ],
        "firm_reply_email": "firm1@yandex-team.ru",
        "firm_url": "string",
        "qr": "t=${receipt.qrDt}&s=${receipt.totalSum}&fn=${receipt.fn}&i=${receipt.id?c}&fp=${receipt.fp}&n=${receipt.receiptType.code?c}"
    }
}
