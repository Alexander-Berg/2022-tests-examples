<?xml version="1.0"?>
<fop version="1.0">
    <strict-configuration>true</strict-configuration>
    <base>.</base>

    <source-resolution>72</source-resolution>
    <target-resolution>72</target-resolution>

    <default-page-settings height="11.00in" width="8.50in"/>

    <renderers>
        <renderer mime="application/pdf">
            <filterList>
                <value>flate</value>
            </filterList>

            <fonts>
                <!-- ArialGeo -->
                <font embed-url="../../fop-resources/fonts/ArialGeo/ArialGeo-Regular.ttf">
                    <font-triplet name="EmbeddedArialGeo" style="normal" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialGeo/ArialGeo-Italic.ttf">
                    <font-triplet name="EmbeddedArialGeo" style="italic" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialGeo/ArialGeo-Bold.ttf">
                    <font-triplet name="EmbeddedArialGeo" style="normal" weight="bold"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialGeo/ArialGeo-Bold-Italic.ttf">
                    <font-triplet name="EmbeddedArialGeo" style="italic" weight="bold"/>
                </font>

                <!-- ArialUnicode -->
                <font embed-url="../../fop-resources/fonts/ArialUnicode/ArialUnicode-Regular.ttf">
                    <font-triplet name="EmbeddedArialUnicode" style="normal" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialUnicode/ArialUnicode-Italic.ttf">
                    <font-triplet name="EmbeddedArialUnicode" style="italic" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialUnicode/ArialUnicode-Bold.ttf">
                    <font-triplet name="EmbeddedArialUnicode" style="normal" weight="bold"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArialUnicode/ArialUnicode-Bold-Italic.ttf">
                    <font-triplet name="EmbeddedArialUnicode" style="italic" weight="bold"/>
                </font>

                <!-- ArmTimesST -->
                <font embed-url="../../fop-resources/fonts/ArmTimesST/ArmTimesST-Regular.ttf">
                    <font-triplet name="EmbeddedArmTimesST" style="normal" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArmTimesST/ArmTimesST-Italic.ttf">
                    <font-triplet name="EmbeddedArmTimesST" style="italic" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArmTimesST/ArmTimesST-Bold.ttf">
                    <font-triplet name="EmbeddedArmTimesST" style="normal" weight="bold"/>
                </font>
                <font embed-url="../../fop-resources/fonts/ArmTimesST/ArmTimesST-Bold-Italic.ttf">
                    <font-triplet name="EmbeddedArmTimesST" style="italic" weight="bold"/>
                </font>

                <!-- Asap -->
                <font embed-url="../../fop-resources/fonts/Asap/Asap-Regular.otf">
                    <font-triplet name="EmbeddedAsap" style="normal" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/Asap/Asap-Italic.otf">
                    <font-triplet name="EmbeddedAsap" style="italic" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/Asap/Asap-Bold.otf">
                    <font-triplet name="EmbeddedAsap" style="normal" weight="bold"/>
                </font>
                <font embed-url="../../fop-resources/fonts/Asap/Asap-Bold-Italic.otf">
                    <font-triplet name="EmbeddedAsap" style="italic" weight="bold"/>
                </font>

                <!-- TimesNewRoman -->
                <font embed-url="../../fop-resources/fonts/TimesNewRoman/TimesNewRoman-Regular.ttf">
                    <font-triplet name="EmbeddedTimesNewRoman" style="normal" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/TimesNewRoman/TimesNewRoman-Italic.ttf">
                    <font-triplet name="EmbeddedTimesNewRoman" style="italic" weight="normal"/>
                </font>
                <font embed-url="../../fop-resources/fonts/TimesNewRoman/TimesNewRoman-Bold.ttf">
                    <font-triplet name="EmbeddedTimesNewRoman" style="normal" weight="bold"/>
                </font>
                <font embed-url="../../fop-resources/fonts/TimesNewRoman/TimesNewRoman-Bold-Italic.ttf">
                    <font-triplet name="EmbeddedTimesNewRoman" style="italic" weight="bold"/>
                </font>
            </fonts>
        </renderer>

        <renderer mime="text/xml"/>
    </renderers>
</fop>
