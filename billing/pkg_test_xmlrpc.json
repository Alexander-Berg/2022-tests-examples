{
  "meta": {
    "name": "yb-balance-test-xmlrpc",
    "maintainer": "Igor Beketov <igogor@yandex-team.ru>",
    "description": "Balance test-xmlrpc servant files separated for —Åonvenience"
  },
  "data": [
    {
      "source": {
        "type": "ARCADIA",
        "path": "billing/balance/test_xmlrpc",
        "files": [
          "balance-test-xmlrpc-ping"
        ]
      },
      "destination": {
        "path": "/usr/bin/"
      }
    },
    {
      "source": {
        "type": "SYMLINK"
      },
      "destination": {
        "target": "/usr/bin/balance-binary",
        "path": "/usr/bin/yb-test-xmlrpc"
      }
    },
    {
      "source": {
        "type": "SYMLINK"
      },
      "destination": {
        "path": "/etc/init.d/balance-test-xmlrpc",
        "target": "/lib/init/upstart-job"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "billing/balance/test_xmlrpc",
        "files": [
          "servant.cfg.xml",
          "servant.env.sh"
        ]
      },
      "destination": {
        "path": "/etc/yandex/balance-test-xmlrpc/"
      }
    },
    {
      "source": {
        "type": "ARCADIA",
        "path": "billing/balance/test_xmlrpc/upstart/balance-test-xmlrpc.conf"
      },
      "destination": {
        "path": "/etc/init/"
      }
    }
  ]
}
