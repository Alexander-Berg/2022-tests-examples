<HtmlParser>
    <Zones>
        title : title
        address : address
        anchor : a/link
        z_keywords : div/s_keyword
        z_text : div/s_text
        z_title : _/s_title
    </Zones>
    <Attributes>
        _ : LITERAL/meta._
        link : URL,anchor/a.href
        link : URL,any/frame.src,iframe.src,area.href
        link : URL/link._
        robots : LITERAL,doc,parse_meta_robots,ignore/meta.robots
        refresh : URL,doc,parse_http_refresh,ignore/meta.refresh
        style : URL/link.stylesheet
        profile : URL/head.profile
        script : URL,any/script.src
        image : URL,any/img.src
        applet : URL,any/applet.code,applet.object
        object : URL,any/object.data,object.classid
        abstract : LITERAL/meta.description        
        hint : LITERAL,any/img.alt,area.alt
        tooltip : LITERAL,any/_.title
        s_keyword : LITERAL/div.data-help-keywords
        s_text : LITERAL,doc,,ignore/div.data-help-text
        s_title : LITERAL,doc,,ignore/_.data-help-title
    </Attributes>
</HtmlParser>