<uwsgi>
  <socket>/var/run/indexer_proxy/uwsgi.sock</socket>
  <listen>1000</listen>
  <master/>
  <vacuum/>
  <processes>24</processes>
  <pythonpath>/usr/libexec/indexer_proxy/</pythonpath>
  <env>INDEXER_PROXY_SETTINGS=config.py</env>
  <module>indexer_proxy</module>
  <callable>app</callable>
  <logto>/var/log/indexer-proxy/uwsgi.log</logto>
  <log-4xx/>
  <log-5xx/>
</uwsgi>
