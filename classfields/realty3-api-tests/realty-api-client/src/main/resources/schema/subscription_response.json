  "properties": {
    "response": {
      "type": "object",
      "properties": {
        "request": {
          "type": "object",
          "properties": {
            "httpQuery": {
              "type": "string"
            }
          }
        },
        "state": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "delivery": {
          "type": "object",
          "properties": {
            "push": {
              "type": "object",
              "properties": {
                "token": {
                  "type": "string"
                },
                "period": {
                  "type": "integer"
                },
                "platform": {
                  "type": "string"
                }
              }
            }
          }
        },
        "user": {
          "type": "string"
        },
        "view": {
          "type": "object",
          "properties": {
