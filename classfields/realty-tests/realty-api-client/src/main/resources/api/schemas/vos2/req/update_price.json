{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "request": {
      "type": "object",
      "properties": {
        "userIp": {
          "type": "string"
        },
        "userAgent": {
          "type": "string"
        },
        "proxyIp": {
          "type": "string"
        },
        "flashCookie": {
          "type": "string"
        },
        "yandexuid": {
          "type": "string"
        }
      }
    },
    "price": {
      "type": "object",
      "properties": {
        "value": {
          "type": "integer"
        },
        "currency": {
          "type": "string"
        },
        "period": {
          "type": "string"
        },
        "unit": {
          "type": "string"
        },
        "installment": {
          "type": "integer"
        },
        "installmentRate": {
          "type": "integer"
        },
        "mortgageBanks": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "dealStatus": {
          "type": "string"
        },
        "mls": {
          "type": "boolean"
        },
        "mlsOffer": {
          "type": "integer"
        },
        "dealType": {
          "type": "string"
        },
        "taxationForm": {
          "type": "string"
        },
        "commission": {
          "type": "integer"
        },
        "securityPayment": {
          "type"  : "integer"
        }
      }
    }
  }
}