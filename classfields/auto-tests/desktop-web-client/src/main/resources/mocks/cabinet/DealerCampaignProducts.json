    {
      "deepEquals": {
        "path": "/1.0/dealer/campaign/products",
        "method": "GET",
        "query": {
          "product": [
            "trade-in-request:cars:used",
            "trade-in-request:cars:new"
          ]
        }
      }
    }
  ],
  "responses": [
    {
      "is": {
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "status": "SUCCESS",
          "items": [
            {
              "campaign": {
                "is_active": true,
                "is_enabled": true
              },
              "cost": 700,
              "product": "trade-in-request:cars:new",
              "is_editable": false
            },
            {
              "cost": 700,
              "campaign": {
                "is_enabled": false,
                "inactive_reason": "MANUALLY_DISABLED",
                "is_active": false
              },
              "is_editable": true,
              "product": "trade-in-request:cars:used"
            }
          ]
        },
        "statusCode": 200
