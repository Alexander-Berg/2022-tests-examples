<?xml version="1.0" ?>
<yandex>
    <zookeeper>
        <node>
            <host>zookeeper</host>
            <port>33234</port>
        </node>
        <session_timeout_ms>30000</session_timeout_ms>
        <operation_timeout_ms>10000</operation_timeout_ms>
    </zookeeper>
    <macros>
        <shard>01</shard>
        <replica>test.yandex.ru</replica>
        <cluster>test_cluster</cluster>
    </macros>
    <remote_servers>
        <test_cluster>
            <shard>
                <!-- replication by ch -->
                <internal_replication>true</internal_replication>
                <replica>
                    <host>localhost</host>
                    <port>9000</port>
                </replica>
            </shard>
        </test_cluster>
    </remote_servers>
    <keep_alive_timeout>15</keep_alive_timeout>
</yandex>
