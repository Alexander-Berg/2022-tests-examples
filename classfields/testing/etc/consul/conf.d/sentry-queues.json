{
  "service": {
    "name": "sentry-queues-metrics",
    "port": 8080,
    "enable_tag_override": false,
    "tags": ["metrics","bridge"],
    "checks": [
      {
        "id": "sentry-queues-metrics",
        "http": "http://localhost:8080/ping",
