{
  "service": {
    "name": "sentry",
    "address": "{{ ansible_default_ipv6.address }}",
    "port": 10222,
    "enable_tag_override": false,
    "tags": ["bridge","domain-sentry.test.vertis.yandex.net","domain-sentry.vrts-slb.test.vertis.yandex.net","noshiva"],
    "checks": [
      {
        "id": "sentry",
        "http": "http://localhost:9090/ping",
        "method": "GET",
        "interval": "5s",
        "timeout": "1s"
      }
    ]
  }
}
