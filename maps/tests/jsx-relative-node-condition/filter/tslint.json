{
	"rules": {
		"jsx-relative-node-condition": [true, [
			{
				"filter": "^S1|F1$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"filter": {
								"type": "tag"
							},
							"existence": "exists"
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "foo",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"filter": {
								"type": "not-tag"
							},
							"existence": "exists"
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "bar",
								"validator": "exists"
							}
						]
					}
				]
			},
			{
				"filter": "^S2|F2$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"filter": {
								"type": "tag",
								"regex": "foo"
							},
							"existence": "exists"
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "foo",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"filter": {
								"type": "not-tag",
								"regex": "Bar"
							},
							"existence": "exists"
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "bar",
								"validator": "exists"
							}
						]
					}
				]
			}
		]]
	}
}
