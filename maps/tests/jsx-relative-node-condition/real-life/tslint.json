{
	"rules": {
		"jsx-relative-node-condition": [true, [
			{
				"filter": "^AnalyticsWrapper$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"existence": "exists",
							"filter": {
								"type": "tag"
							},
							"condition": {
								"type": "attribute",
								"attributeName": "onClick",
								"validator": "exists"
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "logClick",
								"validator": "exists"
							}
						]
					}
				]
			}
		]]
	}
}
