{
	"rules": {
		"jsx-relative-node-condition": [true, [
			{
				"filter": "^S|F$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"filter": "foo",
							"condition": {
								"type": "child",
								"existence": "does-not-exist"
							}
						},
						"thenThat": [
							{
								"type": "child",
								"filter": "bar",
								"condition": {
									"type": "child",
									"existence": "exists"
								}
							}
						]
					}
				]
			}
		]]
	}
}
