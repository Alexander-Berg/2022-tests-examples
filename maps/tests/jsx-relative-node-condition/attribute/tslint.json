{
	"rules": {
		"jsx-relative-node-condition": [true, [
			{
				"filter": "^S|F$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"condition": {
								"type": "attribute",
								"attributeName": "foo",
								"validator": "exists"
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "foo",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"condition": {
								"type": "attribute",
								"attributeName": "baz",
								"validator": "does-not-exist"
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "baz",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"condition": {
								"type": "attribute",
								"attributeName": "bar",
								"validator": {
									"type": "regex",
									"regex": "fooboo$"
								}
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "bar",
								"validator": "exists"
							}
						]
					}
				]
			}
		]]
	}
}
