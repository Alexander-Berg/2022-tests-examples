{
	"rules": {
		"jsx-relative-node-condition": [true, [
			{
				"filter": "^S|F$",
				"conditions": [
					{
						"ifThis": {
							"type": "child",
							"existence": "exists"
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "foo",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"existence": "exists",
							"condition": {
								"type": "child",
								"existence": "exists"
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "fooDeep",
								"validator": "exists"
							}
						]
					},
					{
						"ifThis": {
							"type": "child",
							"existence": "exists",
							"condition": {
								"type": "child",
								"existence": "exists",
								"condition": {
									"type": "child",
									"existence": "exists"
								}
							}
						},
						"thenThat": [
							{
								"type": "attribute",
								"attributeName": "fooSuperDeep",
								"validator": "exists"
							}
						]
					}
				]
			}
		]]
	}
}
