{
    "layers": [
        {
            "id": "streetview",
            "minzoom": 9,
            "maxzoom": 21,
            "source": "stv",
            "source-layer": "streets",
            "type": "line",
            "visible": true,
            "style": {
                "line-width": {
                    "stops": [
                        [
                            6,
                            0.5
                        ],
                        [
                            10,
                            0.9
                        ],
                        [
                            13,
                            1.7
                        ],
                        [
                            15,
                            2.5
                        ],
                        [
                            17,
                            3.5
                        ]
                    ],
                    "type": "interval"
                },
                "line-color": "color:stv_line",
                "line-opacity": 1,
                "line-cap": "round",
                "line-join": "bevel",
                "line-offset": 0,
                "line-border-width": 1.5,
                "line-border-color": "color:stv_outline",
                "line-border-opacity": 1,
                "line-pattern-scale": 1,
                "line-arrow-span": 0,
                "line-arrow-gap": 0,
                "line-arrow-head-width": 0
            }
        }
    ],
    "library": {
        "colors": [
            {
                "id": "stv_line",
                "values": {
                    "basic": "#6971f6ff",
                    "night": "#665edeff"
                }
            },
            {
                "id": "stv_outline",
                "values": {
                    "basic": "#d8daf8ff",
                    "night": "#26225cff"
                }
            }
        ],
        "resources": []
    }
}