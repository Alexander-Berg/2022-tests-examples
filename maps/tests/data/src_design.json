{
    "layers": [
        {
            "id": "Background",
            "minzoom": 0,
            "maxzoom": 21,
            "source": "map",
            "source-layer": "background",
            "type": "background",
            "visible": true,
            "style": {
                "background-color": "color:land",
                "background-opacity": 1
            }
        },
        {
            "id": "poi",
            "minzoom": 15,
            "maxzoom": 21,
            "filter": [
                "all",
                [
                    "in",
                    "group",
                    "other"
                ]
            ],
            "source": "map",
            "source-layer": "poi",
            "type": "label",
            "visible": true,
            "style": {
                "label-target": "point",
                "text-field": "name",
                "text-color": "color:poi_label",
                "text-opacity": 1,
                "text-font": "YSText-Medium.ttf",
                "text-size": 7.5,
                "icon-image": "rc:poi_{icon}_(24|18)",
                "icon-scale": 1,
                "selected-icon-image": "rc:pin_{icon}",
                "selected-icon-scale": 1,
                "text-optional": true
            }
        },
        {
            "id": "bld_residental",
            "minzoom": 16,
            "maxzoom": 21,
            "filter": [
                "all",
                [
                    "!in",
                    "ft_type_name",
                    "urban-industrial",
                    "urban-structure",
                    "urban-transport"
                ]
            ],
            "source": "map",
            "source-layer": "building",
            "type": "fill-extrusion",
            "visible": true,
            "style": {
                "fill-extrusion-height-field": "height",
                "fill-extrusion-default-height": 4,
                "fill-extrusion-color": "color:bld",
                "fill-extrusion-frame-color": "color:bld_out",
                "fill-extrusion-opacity": 0.8,
                "fill-extrusion-frame-width": 0,
                "fill-extrusion-frame-opacity": 1
            }
        },
        {
            "id": "tmp_disabled",
            "minzoom": 10,
            "maxzoom": 21,
            "source": "map",
            "source-layer": "poi",
            "type": "label",
            "visible": false,
            "style": {
                "label-target": "point",
                "text-field": "name",
                "text-color": "color:poi_label",
                "text-opacity": 1,
                "text-font": "YSText-Medium.ttf",
                "text-size": 9
            }
        },
        {
            "id": "events",
            "minzoom": 8,
            "maxzoom": 21,
            "source": "trf",
            "source-layer": "trfe",
            "type": "label",
            "visible": true,
            "style": {
                "label-target": "point",
                "text-font": "YSText-Light.ttf",
                "text-size": 0,
                "icon-image": "rc:event_poi_{icons}",
                "icon-scale": 1
            }
        },
        {
            "id": "traffic_jams",
            "minzoom": 7,
            "maxzoom": 21,
            "source": "trf",
            "source-layer": "trf",
            "type": "line",
            "visible": true,
            "style": {
                "line-width": 2,
                "line-color": "color:jams_green",
                "line-pattern": {
                    "property": "severity",
                    "type": "interval",
                    "default": {
                        "stops": [
                            [
                                14,
                                ""
                            ],
                            [
                                17,
                                "rc:green"
                            ]
                        ]
                    },
                    "stops": [
                        [
                            {
                                "zoom": 14,
                                "value": "hard"
                            },
                            ""
                        ],
                        [
                            {
                                "zoom": 16,
                                "value": "hard"
                            },
                            "rc:red"
                        ]
                    ]
                }
            }
        },
        {
            "id": "traffic_tmp",
            "minzoom": 7,
            "maxzoom": 21,
            "source": "trf",
            "source-layer": "trf",
            "type": "line",
            "visible": false,
            "style": {
                "line-width": 3,
                "line-pattern": "rc:jams_pattern"
            }
        }
    ],
    "library": {
        "colors": [
            {
                "id": "white",
                "values": {
                    "basic": "#ffffff",
                    "night": "#000000"
                }
            },
            {
                "id": "jams_green",
                "values": {
                    "basic": "#0abe0aff",
                    "night": "#57ba38ff"
                }
            },
            {
                "id": "land",
                "values": {
                    "basic": "#fffef7ff",
                    "night": "#1d2430ff"
                }
            },
            {
                "id": "poi_label",
                "values": {
                    "basic": "#7a7f85ff",
                    "night": "#9696a3ff"
                }
            },
            {
                "id": "bld",
                "values": {
                    "basic": "#f4efe1",
                    "night": "#283143ff"
                }
            },
            {
                "id": "bld_out",
                "values": {
                    "basic": "#d1cec7",
                    "night": "#000000"
                }
            }
        ],
        "resources": [
            {
