{
	"rules": {
		"function-name-signature-condition": [true, [{
			"name": "foo",
			"restrictions": [{
				"type": "class",
				"name": "Baz$"
			}],
			"returnTypes": ["Foo"]
		}, {
			"name": "bar",
			"restrictions": [{
				"type": "class",
				"extends": "Boz$"
			}],
			"returnTypes": ["Bar"]
		}]]
	}
}
