{
	"rules": {
		"default-import-naming": [
			true,
			{
				"filter": "^foo",
				"options": [[{"exact": "Bar"}]]
			},
			{
				"filter": ".foo$",
				"options": [[{"from": "\\..*", "to": ""}, "camelCase"]]
			},
			{
				"filter": ".bar$",
				"options": [[{"from": "\\..*", "to": ""}, "camelCase", {"from": ".$", "to": "$&Url"}]]
			}
		]
	}
}
