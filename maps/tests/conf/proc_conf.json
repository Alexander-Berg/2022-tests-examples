{
    "models": [
        { "name": "MODEL_A", "config": "/maps/analyzer/sandbox/eta_prediction/train_model/tests/conf/model_a_conf.json" },
        { "name": "MODEL_B", "config": "/maps/analyzer/sandbox/eta_prediction/train_model/tests/conf/model_b_conf.json" }
    ],
    "train": {
        "train_pools": "//pools",
        "yt_token_in_nirvana": "test",
        "days_back": 30,
        "minimal_count": 1,

        "default_params": {
            "loss_function": "RMSE",
            "iterations": 10000,
            "learning_rate": 0.01,
            "depth": 8
        },
        "params_override": {}
    },
    "check": {
        "tracks": "//sanity_check",
        "filtering": {
            "min_total_length": 3000,
            "max_total_length": 100000
        },
        "re_threshold": 1.2
    },
    "upload": {
        "model_pools": "//models",
        "common_datasets_logs": "//models/common_datasets_logs",
        "expiration_period": 30,
        "model_filename": "matrixnet.info",
        "config_filename": "config.meta",
        "column_description_filename": "column_description.tsv",

        "common_datasets": [
            {
                "name": "models-common-dataset",
                "datasets": "//models/common_datasets",
                "models": [ "MODEL_A", "MODEL_B"],
                "defaults": { "USERS": "MODEL_A", "TAXI": "MODEL_A", "FIXPRICE": "MODEL_A","FUTURE": "MODEL_B"}
            }
        ]
    }
}
