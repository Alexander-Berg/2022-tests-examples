{
    "paths": {
        "Assessors": {
            "USERS": "//analyzer/data/assessors"
        },
        "Common": {
            "TRAVEL_TIMES": "//analyzer/data/travel_times",
            "ROUTER_EVENTS_LOGS": "//analyzer/data/events"
        },
        "AssessorsWithJams": {
            "USERS": "//analyzer/data/training_track",
            "USERS_TRAVEL_TIMES": "//analyzer/data/training_travel_times",
            "FIXPRICE": "//analyzer/data/fixprice_training_tracks"
        }
    },

    "training_tracks": {
        "constants": {
            "jams_expire_age": 60
        },
        "sources": {
            "assessors": "USERS",
            "days_back": 30
        },
        "target": "USERS",
        "expiration_period": 30
    },
    "training_travel_times": {
        "constants": {
            "jams_expire_age": 60
        },
        "sources": {
            "assessors": "TRAVEL_TIMES",
            "days_back": 30
        },
        "split": "router_events",
        "target": "USERS_TRAVEL_TIMES",
        "expiration_period": 30
    },
    "fixprice_training_tracks": {
        "constants": {
            "jams_expire_age": 60
        },
        "sources": {
            "assessors": "USERS",
            "order_stats": "//analyzer/data/order_stats",
            "days_back": 30
        },
        "split": "fixprice_orders",
        "target": "FIXPRICE",
        "expiration_period": 30
    }
}
