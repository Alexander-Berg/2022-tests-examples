<?xml version="1.0" encoding="utf-8"?>
<lego:page xmlns:lego="https://lego.yandex-team.ru">
    <lego:l-head>
        <lego:b-head-logo>
            <lego:b-head-name lego:type="logo">сервис</lego:b-head-name>
        </lego:b-head-logo>
        <lego:b-head-tabs type="search-and-content"/>
        <lego:b-head-search>
            <lego:arrow/>
            <lego:b-search action="/search.xml">
                <lego:input>
                    <lego:b-form-input lego:size="16">
                        <lego:input/>
                    </lego:b-form-input>
                    <lego:precise/>
                    <lego:b-suggest url="http://suggest-slovari.yandex.ru/suggest-sl?callback=?" lego:type="gradient"/>
                    <lego:advanced url="/advanced.xml"/>
                </lego:input>
                <lego:button>
                    <lego:b-form-button type="submit" lego:height="26" lego:theme="grey-no-transparent-26"/>
                </lego:button>
            </lego:b-search>
        </lego:b-head-search>
        <lego:b-head-userinfo>
            <lego:b-head-user>
                <lego:b-user/>
            </lego:b-head-user>
            <lego:b-domik lego:type="popup"/>
            <lego:setup url="/tunes/"/>
            <lego:region url="/tunes/">Москва</lego:region>
            <lego:service/>
            <lego:help url="/help/"/>
        </lego:b-head-userinfo>
    </lego:l-head>
</lego:page>
