{
    "garden": {
        "instance": "testing",
        "server_hostname": "core-garden-server.common.testing.maps.yandex.net",
        "ui_hostname": "garden.tst.c.maps.yandex-team.ru"
    },
    "server": {
        "socket": "/tmp/yandex-maps-garden-server.sock",
        "tvm": {
            "tvm_id": "2024855",
            "client_secret": "{{ ENV.SERVER_TVM_CLIENT_SECRET_FROM_YAV }}"
        }
    },
    "flask": {
        "LOGGER_NAME": "flask_app"
    },
    "yt" : {
        "comments": [
            "https://github.yandex-team.ru/yt/python/blob/master/yt/wrapper/default_config.py#L278",
            "https://wiki.yandex-team.ru/yt/userdoc/operations/#obshhieopciidljavsextipovoperacijj"
        ],
        "config": {
            "token": "{{ ENV.YT_TOKEN_FROM_YAV }}",
            "proxy": {
                "url": "hahn.yt.yandex.net",
                "content_encoding": "identity",
                "connect_timeout": 1000,
                "request_timeout": 20000,
                "retries": {
                    "count": 20,
                    "enable": true,
                    "backoff": {
                        "policy": "constant_time",
                        "constant_time": 1000
                    }
                }
            },
            "read_retries": {
                "count": 20,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "write_retries": {
                "count": 20,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "start_operation_retries": {
                "count": 3,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "spec_overrides": {
                "acl": [
                    {
                        "subjects": [
                            "garden-users"
                        ],
                        "action": "allow",
                        "permissions": [
                            "manage",
                            "read"
                        ]
                    }
                ]
            },
            "allow_http_requests_to_yt_from_job": true
        },
        "task_settings": {
            "environment" : {
                "YT_ALLOW_HTTP_REQUESTS_TO_YT_FROM_JOB":"1"
            }
        },
        "configs_ypath": "//home/maps/core/garden/configs/testing",
        "prefix": "//home/maps/core/garden-exp/tst",
        "network_project": "maps_core_garden"
    },
    "mongo": {
        "host": "sas-sp7cqv46oj8xw3bx.db.yandex.net,vla-p53l0eth3t8uvyol.db.yandex.net,sas-pj82weuiy3cszjis.db.yandex.net",
        "port": 27018,
        "dbname": "garden_server",
        "username": "mapsgarden",
        "password": "{{ ENV.MONGO_PASSWORD_SERVER_FROM_YAV }}",
        "replicaset": "rs01",
        "ssl": true,
        "ssl_ca_certs": "/usr/local/share/ca-certificates/YandexInternalRootCA.crt",
        "authSource": "garden_server",
        "tz_aware": true
    },
    "isolated_modules_cache_dir": "unused_path",
    "autostart": {
        "contours": []
    },
    "sandbox": {
        "token": "unused_token"
    }
}
