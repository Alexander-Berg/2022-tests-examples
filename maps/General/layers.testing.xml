<?xml version="1.0" encoding="UTF-8"?>
<layers>
    <settings defaultLocales="" defaultTransparent="false" defaultScaled="false">
        <layerSuffix locale="ru_RU" suffix=""/>
        <layerSuffix locale="en_US" suffix="-local"/>
    </settings>
    <!-- DEFAULT LAYERS -->
    <!-- base map -->
    <layer type="map">
        <file dir="map" filetype="png" urltemplate="http://core-renderer-cache.common.testing.maps.yandex.net/tiles?l=%l&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme&amp;style=%style" maxage="3600" scaled="true" tvmDst="maps_core_renderer_cache"/>
    </layer>
    <layer type="map" dataProvider="osm">
        <file dir="map" filetype="png" versionurl="http://core-renderer-tiles-osm.common.testing.maps.yandex.net/version?format=xml" urltemplate="http://core-renderer-tiles-osm.common.testing.maps.yandex.net/tiles?l=%l&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme&amp;style=%style" maxage="3600" scaled="true" tvmDst="maps_core_renderer_cache"/>
        <copyright>Â© OpenStreetMap</copyright>
    </layer>
    <!-- uber taxi -->
    <layer type="external_taxi">
        <file dir="external_taxi" filetype="png" urltemplate="http://core-renderer-cache.common.testing.maps.yandex.net/tiles?l=%l&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme" maxage="3600" scaled="true" tvmDst="maps_core_renderer_cache"/>
    </layer>
    <!-- satellite map -->
    <layer type="sat">
        <file dir="sat" filetype="jpg" urltemplate="http://core-sat-internal.maps.yandex.net/tiles?l=sat&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang" maxage="3600" theme="dark"/>
    </layer>
    <!-- skeleton map -->
    <layer type="skl">
        <file dir="skl" filetype="png" transparent="true" urltemplate="http://core-renderer-cache.common.testing.maps.yandex.net/tiles?l=%l&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme&amp;style=%style" maxage="3600" scaled="true" tvmDst="maps_core_renderer_cache"/>
    </layer>
    <!-- graph map -->
    <layer type="grph">
        <file dir="grph" filetype="png" transparent="true"/>
    </layer>
    <!-- relief map -->
    <layer type="rel">
        <file dir="rel" filetype="jpg"/>
    </layer>
    <!-- point-cloud -->
    <layer type="pcl">
        <file dir="pcl" filetype="png" transparent="true" deprecated="true"/>
    </layer>


    <!-- NATIVE LANGUAGE LAYERS -->
    <!-- native language map -->
    <layer type="nmap">
        <file dir="nmap" filetype="png"/>
    </layer>
    <!-- native language skeleton map -->
    <layer type="nskl">
        <file dir="nskl" filetype="png" transparent="true"/>
    </layer>

    <!-- STREETVIEW LAYER -->
    <layer type="stv">
        <file dir="stv" filetype="png" transparent="true"/>
    </layer>

    <!-- STREETVIEW AIRSHIP -->
    <layer type="sta">
        <file dir="sta" filetype="png" transparent="true" maxage="3600"/>
    </layer>
    <layer type="stj">
        <file dir="stj" filetype="js" transparent="true"/>
    </layer>

    <!-- GEOINFO LAYERS -->
    <layer type="grd">
        <file dir="grd" filetype="png" transparent="true"/>
    </layer>

    <!-- traffic jams layer -->
    <layer type="trf">
        <file dir="trf" filetype="png" transparent="true" versionurl="http://core-jams-rdr-cache.maps.yandex.net/version?format=xml" urltemplate="http://core-jams-rdr-cache.maps.yandex.net/tiles?l=%l&amp;tm=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme&amp;seed=%seed" maxage="60" scaled="true" tvmDst="maps_core_jams_rdr_cache"/>
    </layer>
    <layer type="trj">
        <file dir="trf" filetype="js"/>
    </layer>

    <!-- road events layer -->
    <layer type="trfe">
        <file dir="trfe" filetype="png" transparent="true" versionurl="http://core-road-events-renderer-internal.maps.yandex.net/version?format=xml" urltemplate="http://core-road-events-renderer-internal.maps.yandex.net/tiles?l=%l&amp;v=%v&amp;x=%x&amp;y=%y&amp;z=%z&amp;lang=%lang&amp;scale=%scale&amp;theme=%theme" maxage="60" scaled="true" tvmDst="maps_core_road_events_renderer"/>
    </layer>
    <layer type="trje">
        <file dir="trfe" filetype="js"/>
    </layer>

    <!-- mobiles-only cities -->
    <layer type="mtrf">
        <file dir="mtrf" filetype="png" transparent="true"/>
    </layer>

    <!-- mobile traffic layer (debug) -->
    <layer type="trfm">
        <file dir="trfm" filetype="png" transparent="true"/>
    </layer>
    <!-- smilink traffic layer (debug) -->
    <layer type="trfs">
        <file dir="trfs" filetype="png" transparent="true"/>
    </layer>


    <!-- HOTSPOTS LAYERS -->

    <!-- photos -->
    <layer type="pht">
        <file dir="pht" filetype="png"/>
    </layer>
    <layer type="phj">
        <file dir="phj" filetype="js"/>
    </layer>


    <!-- MOBILE LAYERS -->
    <!-- mobile map -->
    <layer type="mmap">
        <file dir="mmap" filetype="png"/>
    </layer>
    <!-- mobile skeleton map -->
    <layer type="mskl">
        <file dir="mskl" filetype="png" transparent="true"/>
    </layer>
    <!-- mobile map for VGA dispalays -->
    <layer type="mmapv">
        <file dir="mmapv" filetype="png" transparent="true"/>
    </layer>

    <!-- MOBILE NATIVE LANGUAGE LAYERS -->
    <!-- mobile native language map -->
    <layer type="mnmap">
        <file dir="mnmap" filetype="png"/>
    </layer>
    <!-- mobile native language skeleton map -->
    <layer type="mnskl">
        <file dir="mnskl" filetype="png" transparent="true"/>
    </layer>
    <!-- mobile native language map for VGA dispalays -->
    <layer type="mnmapv">
        <file dir="mnmapv" filetype="png" transparent="true"/>
    </layer>

    <!-- Traffic 1-st april -->
    <layer type="ntrf">
        <file dir="ntrf" filetype="png"/>
    </layer>
    <layer type="ntrj">
        <file dir="ntrj" filetype="js"/>
    </layer>
</layers>
