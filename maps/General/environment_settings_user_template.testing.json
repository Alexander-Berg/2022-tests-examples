{
    "garden": {
        "server_hostname": "core-garden-server.common.testing.maps.yandex.net",
        "ui_hostname": "garden.tst.c.maps.yandex-team.ru",
        "contour": "<<contour_name>>",
        "instance": "testing",
        "tvm_client": {
            "source_client_id": 2025178,
            "destination_client_id": 2024855,
            "token": "{{ ENV.TOOLS_TVM_SECRET_FROM_YAV }}"
        }
    },
    "mds": {
        "internal_host": "http://storage-int.mds.yandex.net:1111",
        "public_host": "https://storage.mds.yandex.net",
        "prefix": "<<contour_name>>",
        "auth": "{{ ENV.MDS_AUTH_HEADER_OFFLINE_CACHES_FROM_YAV }}"
    },
    "file_storage_s3": {
        "commit_to_s3": true,
        "mds_host": "s3.mds.yandex.net",
        "bucket": "maps-garden-files-testing",
        "prefix": "<<contour_name>>",
        "access_key": "{{ ENV.S3_ACCESS_KEY_FROM_YAV }}",
        "secret_key": "{{ ENV.S3_SECRET_KEY_FROM_YAV }}"
    },
    "logs_storage": {
        "type": "s3",
        "bucket": "maps-garden-logs-testing",
        "key_prefix": "<<contour_name>>"
    },
    "postgresql_servers": {
    },
    "s3": {
        "host": "s3.mds.yandex.net",
        "port": 80,
        "access_key": "{{ ENV.S3_ACCESS_KEY_FROM_YAV }}",
        "secret_key": "{{ ENV.S3_SECRET_KEY_FROM_YAV }}"
    },
    "yt_servers": {
        "hahn": {
            "yt_config": {
                "token": "{{ ENV.YT_TOKEN_FROM_YAV }}",
                "proxy": {
                    "url": "hahn.yt.yandex.net"
                },
                "spec_defaults": {
                    "acl": [
                        {
                            "subjects": [
                                "garden-users"
                            ],
                            "action": "allow",
                            "permissions": [
                                "manage",
                                "read"
                            ]
                        }
                    ]
                }
            },
            "proxy": "hahn.yt.yandex.net",
            "prefix": "//home/maps/core/garden-exp/exp/<<contour_name>>",
            "tmp_dir": "//home/maps/core/garden-exp/exp/<<contour_name>>/tmp"
        }
    },
    "yql": {
        "token": "{{ ENV.YQL_TOKEN_FROM_YAV }}"
    },
    "arcanum": {
        "token": "{{ ENV.ARCANUM_TOKEN_FROM_YAV }}"
    },
    "statface": {
        "token": "{{ ENV.STATFACE_TOKEN_FROM_YAV }}"
    },
    "tvm": {
        "client_id": 2012616,
        "secret": "{{ ENV.ECSTATIC_TVM_CLIENT_SECRET_UNSTABLE_FROM_YAV }}"
    },
    "sandbox-default": {
        "group": "MAPS_GARDEN",
        "token": "{{ ENV.SANDBOX_TOKEN_FROM_YAV }}"
    },
    "error_retry_policy": {
        "try_number": 3,
        "start_delay": 5,
        "backoff_multiplier": 2
    },
    "yt": {
        "network_project": "maps_core_garden_unstable"
    }
}
