    "flask": {
        "LOGGER_NAME": "flask_app"
    },
    "releases_monitorings": [
        {
            "contour": "testing",
            "startrek_queue": "TEST"
        }
    ],
    "startrek": {
        "url": "https://st-api.yandex-team.ru/v2",
        "queue": "TEST",
        "add_assignee": false,
        "add_followers": false,
        "token": "{{ ENV.STARTREK_TOKEN_FROM_YAV }}"
    },
    "yt" : {
        "comments": [
            "https://github.yandex-team.ru/yt/python/blob/master/yt/wrapper/default_config.py#L278",
            "https://wiki.yandex-team.ru/yt/userdoc/operations/#obshhieopciidljavsextipovoperacijj"
        ],
        "config": {
            "token": "{{ ENV.YT_TOKEN_FROM_YAV }}",
            "pool": "garden",
            "proxy": {
                "url": "hahn.yt.yandex.net",
                "content_encoding": "identity",
                "connect_timeout": 1000,
                "request_timeout": 20000,
                "retries": {
                    "count": 20,
                    "enable": true,
                    "backoff": {
                        "policy": "constant_time",
                        "constant_time": 1000
                    }
                }
            },
            "read_retries": {
                "count": 20,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "write_retries": {
                "count": 20,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "start_operation_retries": {
                "count": 3,
                "enable": true,
                "backoff": {
                    "policy": "constant_time",
                    "constant_time": 1000
                }
            },
            "spec_overrides": {
                "acl": [
                    {
                        "subjects": [
                            "garden-users"
                        ],
                        "action": "allow",
                        "permissions": [
                            "manage",
                            "read"
                        ]
                    }
                ]
            },
            "allow_http_requests_to_yt_from_job": true
        },
        "task_settings": {
            "environment" : {
                "YT_ALLOW_HTTP_REQUESTS_TO_YT_FROM_JOB":"1"
            }
        },
        "configs_ypath": "//home/maps/core/garden/configs/testing",
        "prefix": "//home/maps/core/garden-exp/tst",
        "common_garden_path": "//home/maps/core/garden-exp"
