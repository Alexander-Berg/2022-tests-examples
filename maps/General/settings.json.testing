        "shown_events_log_filename": "/var/log/yandex/maps/infopoint/infopoint-shown-events-tskv.log",
        "iss_events_log_filename": "/var/log/yandex/maps/infopoint/infopoint-iss-tskv.log",
        "rotation_period": 43200
    },
    "points_density": {
        "radius": 100,
        "limit": 10
    },
    "export_only_moderated_road_events": true,
    "export_only_moderated_comments": true,
    "metrics": {
        "db_metrics_update_period": 600,
        "flush_period": 60,
        "hgram_bounds": {
            "point_moderation_time_ru_ahhh": [
                {
                    "bound": 0.0,
                    "step": 1
                },
                {
                    "bound": 10.0,
                    "step": 10
                },
                {
                    "bound": 60.0,
                    "step": 60
                },
                {
                    "bound": 1200.0,
                    "step": 300
                },
                {
                    "bound": 3600.0,
                    "step": 3600
                },
                {
                    "bound": 28800.0
                }
            ],
            "point_moderation_time_tur_ahhh": "point_moderation_time_ru_ahhh",
            "point_pending_moderation_age_ru_ahhh": "point_moderation_time_ru_ahhh",
            "point_pending_moderation_age_tur_ahhh": "point_moderation_time_ru_ahhh",
            "comment_moderation_timings_ahhh": "point_moderation_time_ru_ahhh"
        }
    },
    "gdpr": {
        "deletion_requests_polling_period": 300,
        "activate_threads": true
    },
    "enable_tvm": true,
    "moderation": {
        "task_pool_threads": 4,
        "task_pool_queue_capacity": 20,
        "retry_interval": 60,
        "max_pending_duration": 7200,
        "retry_task_interval": 60,
        "mock_clean_web": false,
        "clean_web_url": "https://router-prod.clean-web.yandex.net/v2/",
        "clean_web_use_tvm": true,
        "clean_web_connection_timeout_ms": 1000,
        "clean_web_timeout_ms": 2000,
        "clean_web_tries": 2
    },
    "moderation_rules": [
        {
            "predicates": {
                "empty_verdicts": "true"
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_region": [10842],
                "has_any_verdict": ["road_general_talks"],
                "type": "comment",
                "_comment": "Архангельская область"
            },
            "action": "approved"
        },
        {
            "predicates": {
                "has_any_region": [11225],
                "has_any_verdict": ["road_general_talks"],
                "_comment": "Челябинская область"
            },
            "action": "approved"
        },
        {
            "predicates": {
                "has_any_region": [10174],
                "has_any_verdict": ["road_general_talks", "nonroad"],
                "_comment": "Санкт-Петербург и Ленинградская область"
            },
            "action": "approved"
        },
        {
            "predicates": {
                "bbox": [55, 58, 58, 56],
                "has_any_verdict": ["road_question"],
                "_comment": "Область вокруг Перми"
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_verdict": [
                    "road_spam",
                    "text_auto_spam",
                    "text_auto_personal_data",
                    "text_auto_obscene",
                    "text_toloka_insult",
                    "text_toloka_obscene",
                    "text_toloka_rude",
                    "text_toloka_threat_hard",
                    "text_toloka_threat_light",
                    "text_toloka_vulgarity",
                    "text_toloka_law_violation",
                    "text_toloka_policy",
                    "text_toloka_spam",
                    "text_toloka_no_sense",
                    "text_toloka_personal_data",
                    "fraud_message",
                    "emergency_text_auto_delete"
                ]
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_tag": ["chat", "local_chat"],
                "has_any_verdict": ["empty_text"]
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "type": "comment",
                "has_any_verdict": ["empty_text"]
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_tag": ["chat", "local_chat"],
                "type": "comment",
                "has_any_verdict": ["road_general_talks", "nonroad"],
                "has_any_region": [26]
            },
            "action": "approved"
        },
        {
            "predicates": {
                "has_any_tag": ["chat", "local_chat"],
                "has_any_verdict": ["nonroad"],
                "has_any_region": [26]
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_tag": ["chat", "local_chat"],
                "has_any_verdict": ["road_general_talks"],
                "has_any_region": [26],
                "_comment": "rgt for chat for casting chat -> local_chat"
            },
            "action": "approved"
        },
        {
            "predicates": {
                "has_any_verdict": [
                    "nonroad",
                    "road_general_talks"
                ]
            },
            "action": "disapproved"
        },
        {
            "predicates": {
                "has_any_verdict": [
                    "road_accident",
                    "road_reconstruction",
                    "road_police",
                    "road_jams",
                    "road_detour",
                    "road_question",
                    "road_ask_for_help",
                    "road_place",
                    "road_time",
                    "road_other",
                    "empty_text"
                ]
            },
            "action": "approved"
        }
    ]
}
