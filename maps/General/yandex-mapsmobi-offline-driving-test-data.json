{
    "meta": {
        "name": "yandex-mapsmobi-offline-driving-test-data",
        "maintainer": "Petr Prokhorenkov <eeight@yandex-team.ru>",
        "description": "Test data for offline driving",
        "version": "1.3.0-0"
    },
    "build": {
        "targets": [
            "maps/routing/offline-data-tester"
        ]
    },
    "data": [
        {
            "source": {
                "type": "BUILD_OUTPUT",
                "path": "maps/routing/offline-data-tester",
                "files": ["*"]
            },
            "destination": {
                "path": "/share/yandex/maps/mapkit/driving/offline/"
            }
        }
    ]
}
