{
  "name": "e2e-tests",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "workspaces": [
    "../../../cli-tools/*",
    "../../../packages/*"
  ],
  "scripts": {
    "e2e:generate-data": "NODE_PATH=./src TS_NODE_TRANSPILE_ONLY=true ts-node ./src/generating-data/generate.ts",
    "e2e:remove-data": "NODE_PATH=./src TS_NODE_TRANSPILE_ONLY=true ts-node ./src/generating-data/remove.ts",
    "get-secrets": "fetch-secrets",
    "precypress-open": "npm run e2e:generate-data",
    "cypress-open": "cypress open",
    "postcypress-open": "npm run e2e:remove-data",
    "generate-report": "npm run mochawesome-merge && npm run marge && npm run copy-json-report",
    "copy-json-report": "cp mochawesome.json mochawesome-report/results.json",
    "mochawesome-merge": "npx mochawesome-merge \"cypress/results/*.json\" > mochawesome.json",
    "marge": "npx marge mochawesome.json",
    "postmarge": "TS_NODE_TRANSPILE_ONLY=true ts-node ./cypress/debug.ts",
    "cypress-run:wrap": "cypress run",
    "cypress-run": "TS_NODE_TRANSPILE_ONLY=true ts-node ./cypress/index.ts",
    "e2e:ci": "NODE_ENV=production npm run cypress-run",
    "clean": "rm -rf ./node_modules/",
    "tsc": "tsc --noEmit",
    "pre-push": "npm run tsc"
  },
  "devDependencies": {
    "@b2bgeo/configs": "*",
    "@b2bgeo/yav": "*",
    "@types/bluebird": "3.5.32",
    "@types/chai": "4.2.14",
    "@types/crypto-js": "3.1.47",
    "@types/cypress-cucumber-preprocessor": "4.0.0",
    "@types/fs-extra": "9.0.12",
    "@types/lodash": "4.14.168",
    "@types/node-fetch": "2.5.7",
    "@types/resolve": "1.20.0",
    "@types/yandex-maps": "2.1.21",
    "@yandex-int/tokenator": "0.15.1",
    "bluebird": "3.7.2",
    "chai": "^4.2.0",
    "crypto-js": "4.0.0",
    "cypress-file-upload": "5.0.8",
    "cypress-multi-reporters": "1.5.0",
    "fs-extra": "10.0.0",
    "generate-data": "file:../generate-data",
    "mocha-junit-reporter": "2.0.0",
    "mochawesome": "6.2.2",
    "mochawesome-merge": "4.2.0",
    "mochawesome-report-generator": "5.2.0",
    "moment": "2.29.0",
    "node-fetch": "2.6.1",
    "qs": "6.9.4"
  },
  "dependencies": {
    "@yandex-int/tus-client": "0.5.1",
    "concurrently": "5.3.0",
    "cypress": "9.5.2",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-real-events": "1.7.0",
    "date-fns": "2.28.0",
    "dotenv": "8.2.0",
    "lodash": "4.17.20",
    "read-excel-file": "5.2.28",
    "resolve": "1.22.0",
    "xlsx": "0.14.0"
  }
}
