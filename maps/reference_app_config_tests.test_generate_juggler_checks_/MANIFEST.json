{
   "version": 1,
    "checks": [
        {
            "check_script": "/usr/sbin/yacare",
            "args": ["check", "yacare-testapp", "alive"],
            "services": ["yacare-testapp-alive"],
            "run_always": true,
            "interval": 300,
            "timeout": 60
        },
        {
            "check_script": "/usr/sbin/yacare",
            "args": ["check", "yacare-testapp", "i"],
            "services": ["yacare-testapp-i"],
            "run_always": true,
            "interval": 300,
            "timeout": 60
        },
        {
            "check_script": "/usr/sbin/yacare",
            "args": ["check", "yacare-testapp", "ii"],
            "services": ["yacare-testapp-ii"],
            "run_always": true,
            "interval": 300,
            "timeout": 60
        },
        {
            "check_script": "/usr/sbin/yacare",
            "args": ["check", "yacare-testapp", "load"],
            "services": ["yacare-testapp-load"],
            "run_always": true,
            "interval": 300,
            "timeout": 60
        }
    ]
}
