            }
        },
        {
            "source": {
                "type": "ARCADIA",
                "path": "maps/b2bgeo/mvrp_solver/backend/integration_tests/aws_docker",
                "files": [
                    "Dockerfile"
                ]
            },
            "destination": {
                "path": "/"
            }
        }
    ]
}
