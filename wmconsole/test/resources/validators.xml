        <constructor-arg value="false"/>
        <constructor-arg ref="mustBeVideoPropertyList"/>
    </bean>
    <bean id="videoFieldValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.MustBeFieldsMicrodataValidator">
        <constructor-arg value="false"/>
        <constructor-arg>
            <list>
                <value>video</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="musicVideoValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.MustBeFieldsMicrodataValidator">
