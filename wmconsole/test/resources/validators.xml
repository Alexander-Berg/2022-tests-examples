    <!-- Product with Offer validators -->

    <bean id="productValidatorWrapper" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg type="java.lang.String" value="Product"/>
