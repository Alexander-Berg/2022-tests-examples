<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util.xsd">

    <!-- Schema.org validator begins -->

    <bean id="schemaBasicClassmap" factory-method="getSchemaClassMap" class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils">
        <constructor-arg value="rdfs.html"/>
    </bean>


    <bean id="yandexedSchemaClassMap" factory-method="readAndMerge" class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils">
        <constructor-arg ref="schemaBasicClassmap"/>
        <constructor-arg>
            <bean class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils" factory-method="getContentFromResource">
                <constructor-arg value="yandex-schema.patch.html"/>
            </bean>
        </constructor-arg>
    </bean>

    <bean id="schemaOrgValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.rdfa.validator.SchemaValidator">
        <constructor-arg ref="yandexedSchemaClassMap"/>
        <property name="vocab" value="schema.org"/>
    </bean>

    <bean id="dataVocabMap" factory-method="getSchemaClassMap" class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils">
        <constructor-arg value="data-vocab.html"/>
    </bean>

    <bean id="dataVocabValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.rdfa.validator.SchemaValidator">
        <constructor-arg ref="dataVocabMap"/>
        <property name="vocab" value="data-vocabulary.org"/>
    </bean>

    <bean id="schemedGoodRelationsClassmap" factory-method="readAndMerge"
          class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils">
        <constructor-arg ref="goodRelationsBasicClassmap"/>
        <constructor-arg>
            <bean class="ru.yandex.webmaster3.core.semantic.schema_org_information_extractor.RDFsSchemaUtils" factory-method="getContentFromResource">
                <constructor-arg value="rdfs.html"/>
            </bean>
        </constructor-arg>
    </bean>

    <bean id="microdataGoodRelationsValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.GoodRelationsValidator">
        <constructor-arg ref="schemedGoodRelationsClassmap"/>
        <property name="vocab" value="purl.org"/>
    </bean>


    <!-- schema.org/Review -->

    <bean id="reviewValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.YandexSOrgReviewMicrodataValidator"/>

    <bean id="businessReviewValidator"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.BusinessReviewCheckingMicrodataValidatorWrapper">
        <constructor-arg>
            <list>
                <ref bean="reviewValidator"/>
            </list>
        </constructor-arg>
    </bean>
    <bean id="autoReviewValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.AutoReviewCheckingMicrodataValidatorWrapper">
        <constructor-arg>
            <list>
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.MustBeFieldsMicrodataValidator">
                    <constructor-arg value="false"/>
                    <constructor-arg>
                        <list>
                            <value>url</value>
                            <value>author/name</value>
                            <value>itemReviewed/model</value>
                            <value>itemReviewed/brand</value>
                        </list>
                    </constructor-arg>
                </bean>
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.AltFieldListsMicrodataValidator">
                    <constructor-arg value="false"/>
                    <constructor-arg>
                        <list>
                            <value>pro</value>
                            <value>contra</value>
                            <value>description</value>
                            <value>reviewBody</value>
                        </list>
                    </constructor-arg>
                </bean>
            </list>
        </constructor-arg>
    </bean>

    <bean id="reviewValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg type="java.lang.String" value="Review"/>
        <constructor-arg>
            <list>
                <!--<ref bean="businessReviewValidator"/>-->

                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                    <constructor-arg ref="businessReviewValidator"/>
                    <constructor-arg value="biz_rev_condition"/>
                </bean>
                <!--<ref bean="autoReviewValidator"/>-->
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                    <constructor-arg ref="autoReviewValidator"/>
                    <constructor-arg value="auto_rev_condition"/>
                </bean>
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.UnsupportedItemReviewedMicrodataValidator"/>
            </list>
        </constructor-arg>
    </bean>

    <!-- schema.org/Place and schema.org/Organization and children -->

    <bean id="orgFieldsValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.MustBeFieldsMicrodataValidator">
        <constructor-arg value="false"/>
        <constructor-arg>
            <list>
                <value>name</value>
                <value>address</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="orgPhoneValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.YaOrgTelMicrodataValidator">
        <constructor-arg value="false"/>
    </bean>

    <bean id="orgCountryValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.UnsupportedCountryMicrodataValidator">
        <property name="countries">
            <list>
                <value>РОССИЯ</value>
                <value>УКРАИНА</value>
                <value>БЕЛАРУСЬ</value>
                <value>БЕЛОРУССИЯ</value>
                <value>КАЗАХСТАН</value>
                <value>BY</value>
                <value>RU</value>
                <value>UA</value>
            </list>
        </property>
    </bean>

    <bean id="orgValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.AggregateMicrodataValidator">
        <constructor-arg>
            <list>
                <ref bean="orgFieldsValidator"/>
                <ref bean="orgPhoneValidator"/>
                <ref bean="orgCountryValidator"/>
            </list>
        </constructor-arg>
    </bean>

    <bean id="placeValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg type="java.lang.String" value="Place"/>
        <constructor-arg>
            <list>
                <!--<ref bean="orgValidator"/>-->
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                    <constructor-arg ref="orgValidator"/>
                    <constructor-arg value="org_condition"/>
                </bean>
            </list>
        </constructor-arg>
    </bean>

    <bean id="orgValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg type="java.lang.String" value="Organization"/>
        <constructor-arg>
            <list>
                <!--<ref bean="orgValidator"/>-->
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                    <constructor-arg ref="orgValidator"/>
                    <constructor-arg value="org_condition"/>
                </bean>
            </list>
        </constructor-arg>
    </bean>

    <!-- schema.org/ScholarlyArticle -->

    <bean id="schoolArtValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.MustBeFieldsMicrodataValidator">
