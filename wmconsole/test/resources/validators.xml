        <constructor-arg type="java.lang.String" value="ScholarlyArticle"/>
        <constructor-arg>
            <list>
                <!--<ref bean="schoolArtValidator"/>-->
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                <constructor-arg ref="schoolArtValidator"/>
                    <constructor-arg value="scholarly_article_condition"/>
