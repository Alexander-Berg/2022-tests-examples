            </list>
        </constructor-arg>
    </bean>
    <bean id="tvShowValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg type="java.lang.String" value="TVShow"/>
        <constructor-arg>
            <list>
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
