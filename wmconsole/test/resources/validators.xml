    <!-- schema.org/ImageObject-->

    <bean id="imgObjValidator" class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.AltFieldListsMicrodataValidator">
        <constructor-arg value="false"/>
        <constructor-arg>
            <list>
                <value>contentUrl</value>
                <value>image</value>
                <value>thumbnail</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="imgObjValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
