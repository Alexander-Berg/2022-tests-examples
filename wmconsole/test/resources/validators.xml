        <constructor-arg>
            <list>
                <value>description</value>
                <value>articleBody</value>
            </list>
        </constructor-arg>
    </bean>

    <bean id="creativeWorkValidatorWrapper"
          class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.SchemaOrgTypeCheckingMicrodataValidatorWrapper">
        <constructor-arg>
            <list>
                <value>CreativeWork</value>
                <value>Article</value>
                <value>BlogPosting</value>
                <value>NewsArticle</value>
                <value>ScholarlyArticle</value>
                <value>MedicalScholarlyArticle</value>
                <value>Blog</value>
                <value>Book</value>
                <value>Diet</value>
                <value>ExercisePlan</value>
                <value>MediaObject</value>
                <value>AudioObject</value>
                <!--<value>ImageObject</value>-->
                <value>MusicVideoObject</value>
                <value>VideoObject</value>
                <value>Painting</value>
                <value>Photograph</value>
                <value>Sculpture</value>
                <value>TVEpisode</value>
                <value>TVSeason</value>
                <value>TVSeries</value>
            </list>
        </constructor-arg>
        <constructor-arg>
            <list>
                <!--<ref bean="schoolArtValidator"/>-->
                <bean class="ru.yandex.webmaster3.core.semantic.semantic_document_parser.microdata.validators.PrefixWrapperMicrodataValidator">
                    <constructor-arg ref="creativeWorkValidator"/>
                    <constructor-arg value="creative_work_condition"/>
                </bean>
            </list>
        </constructor-arg>
    </bean>

