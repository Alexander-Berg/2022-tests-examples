{
  "matcher": {
    "pathRe": "(/.*)?"
  }, 
  "version": "0.0.1", 
  "id": "root", 
  "byDcScheme": {
    "onError": {
      "static": {
        "status": 504
      }
    }, 
    "dcBalancer": {
      "attemptAllDcs": true, 
      "method": "BY_DC_WEIGHT", 
      "weightsSectionId": "by_geo"
    }, 
    "dcs": [
      {
        "backendIds": [
          "abt_dashboard_production.ui.iva"
        ], 
        "name": "iva"
      }, 
      {
        "backendIds": [
          "abt_dashboard_production.ui.myt"
        ], 
        "name": "myt"
      }, 
      {
        "backendIds": [
          "abt_dashboard_production.ui.sas"
        ], 
        "name": "sas"
      }
    ], 
    "balancer": {
      "connectTimeout": "60ms", 
      "backendTimeout": "10s", 
      "maxPessimizedEndpointsShare": 0.2, 
      "attempts": 3, 
      "doNotLimitReattempts": true, 
      "doNotRetryHttpResponses": true
    }
  }
}