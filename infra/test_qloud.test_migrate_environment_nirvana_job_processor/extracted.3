{
  "matcher": {
    "pathRe": "(/api/.*)?"
  }, 
  "version": "0.0.1", 
  "id": "api", 
  "byDcScheme": {
    "onError": {
      "static": {
        "status": 504
      }
    }, 
    "dcBalancer": {
      "attemptAllDcs": true, 
      "method": "BY_DC_WEIGHT", 
      "weightsSectionId": "by_geo"
    }, 
    "dcs": [
      {
        "backendIds": [
          "nirvana_nirvana-job-processor_test.backend.myt"
        ], 
        "name": "myt"
      }, 
      {
        "backendIds": [
          "nirvana_nirvana-job-processor_test.backend.vla"
        ], 
        "name": "vla"
      }
    ], 
    "balancer": {
      "backendTimeout": "0s", 
      "maxPessimizedEndpointsShare": 0.2, 
      "attempts": 0, 
      "doNotLimitReattempts": true, 
      "doNotRetryHttpResponses": true, 
      "backendReadTimeout": "120s"
    }
  }
}