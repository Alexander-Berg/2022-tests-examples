{
  "matcher": {
    "pathRe": "(/.*)?"
  }, 
  "version": "0.0.1", 
  "id": "root", 
  "byDcScheme": {
    "onError": {
      "static": {
        "status": 504
      }
    }, 
    "dcBalancer": {
      "attemptAllDcs": true, 
      "method": "BY_DC_WEIGHT", 
      "weightsSectionId": "by_geo"
    }, 
    "dcs": [
      {
        "backendIds": [
          "metrika_cms_production.frontend.sas"
        ], 
        "name": "sas"
      }, 
      {
        "backendIds": [
          "metrika_cms_production.frontend.vla"
        ], 
        "name": "vla"
      }
    ], 
    "balancer": {
      "connectTimeout": "60ms", 
      "backendTimeout": "10s", 
      "maxPessimizedEndpointsShare": 0.2, 
      "attempts": 3, 
      "doNotLimitReattempts": true, 
      "doNotRetryHttpResponses": true, 
      "backendWriteTimeout": "15s", 
      "backendReadTimeout": "15s"
    }
  }
}